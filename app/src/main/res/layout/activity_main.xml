<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context=".MainActivity">

    <AutoCompleteTextView
        android:id="@+id/auto_complete_edit_text"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/edit"
        android:hint="Enter stock name"
        android:padding="10dp"
        android:textColor="#000"
        android:textColorHint="#708FA0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/symbol"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="160dp"
        android:background="@drawable/circular_textview_drawable"
        android:backgroundTint="#E365F1"
        android:drawableLeft="@drawable/ic_fast_forward_black_24dp"
        android:gravity="center"
        android:textColor="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/high"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="236dp"
        android:background="@drawable/circular_textview_drawable"
        android:backgroundTint="#49C44D"
        android:drawableLeft="@drawable/ic_arrow_upward_black_24dp"
        android:gravity="center"
        android:textColor="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/low"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="160dp"
        android:background="@drawable/circular_textview_drawable"
        android:backgroundTint="#FF0000"
        android:drawableLeft="@drawable/ic_arrow_downward_black_24dp"
        android:gravity="center"
        android:textColor="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/price"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="236dp"
        android:background="@drawable/circular_textview_drawable"
        android:backgroundTint="#2762FC"
        android:drawableLeft="@drawable/ic_attach_money_black_24dp"
        android:gravity="center"
        android:textColor="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/volume"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="88dp"
        android:background="@drawable/circular_textview_drawable"
        android:backgroundTint="#FFBE01"
        android:drawableLeft="@drawable/ic_work_black_24dp"
        android:gravity="center"
        android:textColor="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/open"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true"
        android:layout_marginTop="88dp"
        android:background="@drawable/circular_textview_drawable"
        android:backgroundTint="#EA93F8"
        android:drawableLeft="@drawable/ic_attach_money_black_24dp"
        android:gravity="center"
        android:textColor="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/noofshares"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"


        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:background="@drawable/edit"
        android:hint="No. Of Shares"
        android:inputType="textPersonName"

        android:padding="10dp"
        android:textColor="#000"
        android:textColorHint="#708FA0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/high" />

    <Button


        android:id="@+id/button3"
        android:layout_width="253dp"
        android:layout_height="54dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="390dp"
        android:background="@drawable/edit"
        android:backgroundTint="@color/colorPrimary"
        android:hint="Enter stock name"
        android:padding="10dp"
        android:text="Add to Portfolio"
        android:textColor="#000"
        android:textColorHint="#708FA0"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.0" />

    <Button


        android:id="@+id/viewall"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="510dp"
        android:background="@drawable/edit"
        android:backgroundTint="@color/colorPrimary"
        android:hint="View"
        android:padding="10dp"
        android:text="View All"
        android:textColor="#000"
        android:textColorHint="#708FA0"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.099" />

    <Button


        android:id="@+id/button5"
        android:layout_width="250dp"
        android:layout_height="54dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="456dp"
        android:background="@drawable/edit"
        android:backgroundTint="@color/colorPrimary"
        android:hint="Enter stock name"
        android:padding="10dp"
        android:text="Update"
        android:textColor="#000"
        android:textColorHint="#708FA0"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.471"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        />

    <Button


        android:id="@+id/delete"
        android:layout_width="255dp"
        android:layout_height="50dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="550dp"
        android:background="@drawable/edit"
        android:backgroundTint="@color/colorPrimary"
        android:hint="View"
        android:padding="10dp"
        android:text="Delete"
        android:textColor="#000"
        android:textColorHint="#708FA0"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        app:layout_constraintVertical_bias="0.818" />


</androidx.constraintlayout.widget.ConstraintLayout>